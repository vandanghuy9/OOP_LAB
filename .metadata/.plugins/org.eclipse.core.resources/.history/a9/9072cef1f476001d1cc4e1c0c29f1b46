package hust.soict.dsai.test.book;

public class Book {
	private int id;
	private String title;
	private String category;
	private String listAuthor;
	private int contentLength;
	private static int numberOfBooks =0;
	public Book(String title, String category, String listAuthor,int length) {
		this.id = numberOfBooks;
		numberOfBooks++;
		this.title = title;
		this.category = category;
		this.listAuthor = listAuthor;
		this.contentLength = length;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getCategory() {
		return category;
	}
	public void setCategory(String category) {
		this.category = category;
	}
	public String getListAuthor() {
		return listAuthor;
	}
	public void setListAuthor(String listAuthor) {
		this.listAuthor = listAuthor;
	}
	public int getContentLength() {
		return contentLength;
	}
	public void setContentLength(int contentLength) {
		this.contentLength = contentLength;
	}
	
	
}
