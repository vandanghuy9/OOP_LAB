package hust.soict.dsai.aims.media;

import java.util.ArrayList;
import java.util.List;

public class testMedia {

	public testMedia() {
		// TODO Auto-generated constructor stub
	}
	public static void main(String[] args) {
		List<Media> mediae = new ArrayList<Media>();
		CompactDisc cd = new CompactDisc(1, "Hello", "hello", 10, 50, "hello", "hello");
		DigitalVideoDisc dvd = new DigitalVideoDisc("Hello", "pop","adele", 10, 20f);
		Book book = new Book(1,"Hello","new artist",50f);
		mediae.add(book);
		mediae.add(cd);
		mediae.add(dvd);
		for (Media m: mediae) {
			System.out.println(m.toString());
		}
	}
}
